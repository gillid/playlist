{
  "$schema": "https://ui.shadcn.com/schema.json",
  "style": "new-york",
  "rsc": true,
  "tsx": true,
  "tailwind": {
    "config": "tailwind.config.ts",
    "css": "src/app/(layout)/globals.css",
    "baseColor": "slate",
    "cssVariables": true,
    "prefix": ""
  },
  "iconLibrary": "lucide",
  "aliases": {
    "components": "@/ui/*",
    "ui": "@/ui/*",
    "utils": "@/ui/utils"
  },
  "registries": {
    "@acme": "https://acme.com/r/{name}.json"
  }
}
