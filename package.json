{
  "name": "playlist",
  "version": "0.1.0",
  "private": true,
  "packageManager": "pnpm@10.18.2",
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build --turbopack",
    "start": "next start",
    "lint": "next lint",
    "prepare": "husky",
    "shadcn": "shadcn",
    "db:generate": "dotenv -e .env.development -- prisma generate",
    "db:diff": "dotenv -e .env.development -- prisma migrate dev --create-only",
    "db:migrate": "dotenv -e .env.development -- prisma migrate dev",
    "db:reset": "dotenv -e .env.development -- prisma migrate reset",
    "db:push": "dotenv -e .env.development -- prisma db push",
    "db:studio": "dotenv -e .env.development -- prisma studio",
    "db:seed": "dotenv -e .env.development -- prisma db seed",
    "db:migrate:deploy": "dotenv -e .env.production -- prisma migrate deploy"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.11.0",
    "@faker-js/faker": "^10.1.0",
    "@prisma/client": "^6.18.0",
    "@prisma/extension-accelerate": "^2.0.2",
    "@radix-ui/react-slot": "^1.2.3",
    "@shadcn/ui": "^0.0.4",
    "better-auth": "^1.3.28",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.469.0",
    "next": "15.5.4",
    "pino": "^10.1.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "server-only": "^0.0.1",
    "tailwind-merge": "^3.3.1",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@better-auth/cli": "^1.3.28",
    "@eslint/eslintrc": "^3",
    "@next/eslint-plugin-next": "15.5.4",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "dotenv-cli": "^10.0.0",
    "eslint": "^9",
    "eslint-config-next": "15.5.4",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-react-hooks": "^7.0.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.4",
    "pino-pretty": "^13.1.2",
    "prettier": "^3.6.2",
    "prettier-plugin-multiline-arrays": "^4.0.3",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "prisma": "^6.17.1",
    "shadcn": "^2.0.0",
    "tailwindcss": "^4",
    "tsx": "^4.20.6",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5"
  }
}
